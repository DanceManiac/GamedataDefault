[logic]
active = sr_idle@start

[sr_idle@start]
on_info = {+mar_intro_start} sr_idle@close1 %=disable_level_changer(1025:tips_agroprom_undeground) =disable_level_changer(1026:level_changer_disabled)%

[sr_idle@close1]
on_info = {+agr_actor_inside_9} sr_idle@close2 %=enable_level_changer(1025)%

[sr_idle@close2]
on_info = {+agru_teleport_complete} sr_idle@end %=enable_level_changer(1026) =disable_level_changer(1025:tips_agroprom_undeground2)%

[sr_idle@end]