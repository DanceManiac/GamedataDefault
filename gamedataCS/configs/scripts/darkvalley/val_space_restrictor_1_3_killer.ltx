[logic]
active = sr_idle

[sr_idle]
on_info = {+val_strike_antenna_two_captured} sr_idle@timer

[sr_idle@timer]
on_game_timer = 20 | sr_idle@2 %=create_squad(val_smart_terrain_1_3_killer:val_smart_terrain_1_2)%

[sr_idle@2]
on_game_timer = 10 | sr_idle@3

[sr_idle@3]
on_info = {!squad_exist(val_smart_terrain_1_3_killer)} nil %+val_killer_1_3_dead%
