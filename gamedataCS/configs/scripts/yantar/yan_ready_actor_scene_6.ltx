[logic]
active = sr_idle

[sr_idle] 
on_actor_inside = sr_idle@go1 %+yan_actor_ready_cut6_ok%

[sr_idle@go1]
on_info = {+yan_ready_actor_cut6} sr_idle@go2

[sr_idle@go2]
on_game_timer = 30 | sr_idle@yan_actor_start_to_go %+yan_actor_start_to_go%

[sr_idle@yan_actor_start_to_go]
on_game_timer = 20 | sr_idle@go3 %+yan_ready_actor2_cut6%

[sr_idle@go3]
on_game_timer = 40 | sr_idle@timer %+yan_ready_actor_cut6%

[sr_idle@timer]
on_info = {+yan_ready_perekl_cut6} sr_timer@1

[sr_timer@1]
type = dec
start_value = 241500
on_value = 0 | {-yan_levsha_dead} nil %+yan_stop_spawn_cut6%, nil


