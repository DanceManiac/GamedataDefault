<?xml version="1.0" encoding="windows-1251" ?>
<game_dialogs>

	<!-- Получение продуктов у Сидоровича -->

	<dialog id="rt_courier_dialog">
		<precondition>ogsm_rt_manager.precondition_courier</precondition>
		<phrase_list>
			<phrase id="0">
				<text>st_rt_courier_dialogs_0</text>
				<next>1</next>
			</phrase>
			<phrase id="1">
				<text>st_rt_courier_dialogs_1</text>
				<action>ogsm_rt_manager.action_take_food_packet</action>
				<next>2</next>
			</phrase>
			<phrase id="2">
				<text>st_rt_courier_dialogs_2</text>
				<action>dialogs.break_dialog</action>
			</phrase>
		</phrase_list>
	</dialog>

	<!-- Получение боеприпасов у торговца -->

	<dialog id="rt_squad_courier_dialog">
		<precondition>ogsm_rt_manager.precondition_squad_courier</precondition>
		<phrase_list>
			<phrase id="0">
				<text>st_rt_squad_courier_dialogs_0</text>
				<next>1</next>
			</phrase>
			<phrase id="1">
				<text>st_rt_squad_courier_dialogs_1</text>
				<action>ogsm_rt_manager.action_take_ammo_packet</action>
				<next>2</next>
			</phrase>
			<phrase id="2">
				<text>st_rt_squad_courier_dialogs_2</text>
				<action>dialogs.break_dialog</action>
			</phrase>
		</phrase_list>
	</dialog>

	<!-- Передача боеприпасов командиру отряда -->

	<dialog id="rt_to_squad">
		<precondition>ogsm_rt_manager.squad_courier_has_item</precondition>
		<phrase_list>
			<phrase id="0">
				<text>st_rt_to_squad_dialogs_0</text>
				<next>1</next>
			</phrase>
			<phrase id="1">
				<text>st_rt_to_squad_dialogs_1</text>
				<next>11</next>
			</phrase>
			<phrase id="11">
				<text>st_rt_to_squad_dialogs_11</text>
				<action>ogsm_rt_manager.squad_courier_dlg_action</action>
			</phrase>
		</phrase_list>
	</dialog>

